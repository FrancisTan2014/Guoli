    <div id="wrap" class="animsition">




        <div class="page page-core page-login">

            <div class="text-center">
                <h3 class="text-light text-white">
                    机车运用管控系统
                </h3>
            </div>

            <div class="container w-420 p-15 bg-white mt-40 text-center">


                <h2 class="text-light text-greensea">登 录</h2>

                <form name="form" class="form-validation mt-20" novalidate="">
                    <input type="hidden" name="backUrl" value="@ViewBag.BackUrl" />
                    <div class="form-group">
                        <input name="Account" type="text" class="form-control underline-input" required data-parsley-trigger="change" data-parsley-id="1077" placeholder="账号" maxlength="20" data-parsley-required-message="账号不能为空">
                        <ul class="parsley-errors-list" id="parsley-id-1077"></ul>
                    </div>

                    <div class="form-group">
                        <input name="Password" type="password" placeholder="密码" class="form-control underline-input" data-parsley-trigger="change" data-parsley-id="1078" data-parsley-minlength="6" required data-parsley-minlength-message="密码不能少于六位" data-parsley-required-message="密码不能为空" maxlength="20">
                        <ul class="parsley-errors-list" id="parsley-id-1078"></ul>
                    </div>

                    <div class="form-group text-center mt-20 login-btn">
                        <a href="javascript:void(0);" class="btn btn-greensea b-0 br-2 mr-5" style="width: 60%" id="btnLogin">登 录</a><br />
                        <label class="checkbox checkbox-custom-alt checkbox-custom-sm inline-block">
                            <input type="checkbox" name="Switch"><i></i> 七天免登录
                        </label>
                        <a href="#" id="jump" style="display: none;"></a>
                    </div>
                    @*<div class="alert alert-lightred alert-dismissable hide" id="loginFailedTips">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                        <p>用户名或者密码错误</p>
                    </div>*@
                </form>

