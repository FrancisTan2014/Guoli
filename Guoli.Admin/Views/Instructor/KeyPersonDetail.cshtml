@{
    Layout = "~/Views/Shared/_Layout.cshtml";

    ViewBag.Title = "关键人管理-详情";
    ViewBag.LastPage = "关键人管理列表";
    ViewBag.LastUrl = "/Instructor/KeyPerson";
    ViewBag.ThisPage = "关键人管理详情";
    ViewBag.ThisUrl = "/Instructor/KeyPersonDetail?id=" + ViewBag.Id;
}

<form class="form-horizontal bg-white p-10" role="form">

    <input type="hidden" name="target" value="ViewInstructorKeyPerson" />

    <div class="form-group">
        <label class="col-sm-2 control-label">确定日期：</label>
        <div class="col-sm-10">
            <label class="control-label text-left" name="ConfirmDate"></label>
        </div>
    </div>

    <hr class="line-dashed line-full">

    <div class="form-group">
        <label class="col-sm-2 control-label">关键人：</label>
        <div class="col-sm-10">
            <label class="control-label text-left" name="KeyPersonName"></label>
        </div>
    </div>

    <hr class="line-dashed line-full">

    <div class="form-group">
        <label class="col-sm-2 control-label">包保人：</label>
        <div class="col-sm-10">
            <label class="control-label text-left" name="Name"></label>
        </div>
    </div>

    <hr class="line-dashed line-full">

    <div class="form-group">
        <label class="col-sm-2 control-label">预计解除日期：</label>
        <div class="col-sm-10">
            <label class="control-label text-left" name="ExpectRemoveTime"></label>
        </div>
    </div>

    <hr class="line-dashed line-full">

    <div class="form-group">
        <label class="col-sm-2 control-label">实际解除日期：</label>
        <div class="col-sm-10">
            <label class="control-label text-left" name="ActualRemoveTime"></label>
        </div>
    </div>

    <hr class="line-dashed line-full">

    <div class="form-group">
        <label class="col-sm-2 control-label">关键点：</label>
        <div class="col-sm-10">
            <label class="control-label text-left" name="KeyLocation"></label>
        </div>
    </div>

    <hr class="line-dashed line-full">

    <div class="form-group">
        <label class="col-sm-2 control-label">关键人确定原因：</label>
        <div class="col-sm-10">
            <label class="control-label text-left" name="PersonConfirmReason"></label>
        </div>
    </div>

    <hr class="line-dashed line-full">

    <div class="form-group">
        <label class="col-sm-2 control-label">帮救措施：</label>
        <div class="col-sm-10">
            <label class="control-label text-left" name="HelpMethod"></label>
        </div>
    </div>

    <hr class="line-dashed line-full">

    <div class="form-group">
        <label class="col-sm-2 control-label">转换情况：</label>
        <div class="col-sm-10">
            <label class="control-label text-left" name="Changes"></label>
        </div>
    </div>

    <hr class="line-dashed line-full">

    <div class="form-group">
        <label class="col-sm-2 control-label">关键人解除意见：</label>
        <div class="col-sm-10">
            <label class="control-label text-left" name="PersonRemoveSuggests"></label>
        </div>
    </div>

    <hr class="line-dashed line-full">

    <div class="form-group">
        <label class="col-sm-2 control-label">关键点确定原因：</label>
        <div class="col-sm-10">
            <label class="control-label text-left" name="LocationConfirmReason"></label>
        </div>
    </div>

    <hr class="line-dashed line-full">

    <div class="form-group">
        <label class="col-sm-2 control-label">盯控措施：</label>
        <div class="col-sm-10">
            <label class="control-label text-left" name="ControlMethod"></label>
        </div>
    </div>

    <hr class="line-dashed line-full">

    <div class="form-group">
        <label class="col-sm-2 control-label">落实情况：</label>
        <div class="col-sm-10">
            <label class="control-label text-left" name="ActualControl"></label>
        </div>
    </div>

    <hr class="line-dashed line-full">

    <div class="form-group">
        <label class="col-sm-2 control-label">关键点解除意见：</label>
        <div class="col-sm-10">
            <label class="control-label text-left" name="LocationRemoveSuggests"></label>
        </div>
    </div>

    <hr class="line-dashed line-full">

    <div class="form-group">
        <div class="col-sm-4 col-sm-offset-2">
            <a href="/Instructor/KeyPerson" class="btn btn-lg btn-primary" id="btnBack">返 回</a>
        </div>
    </div>
</form>

<script src="/Scripts/page-scripts/instructor-detail.js"></script>